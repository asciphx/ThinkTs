{
  "apps":
  {
    "name": "ThinkTs",
    "cwd": "./",
    "script": "..ts",
    "exec_interpreter": "C:/nodejs/ts-node",
    "instances": "max",
    "min_uptime": "60s",
    "max_restarts": 30,
    "exec_mode" : "cluster_mode",
    "error_file" : "./.vscode/test-err.log",
    "out_file": "./.vscode/test-out.log",
    "pid_file": "./.vscode/test.pid",
    "watch": false
  }
}
