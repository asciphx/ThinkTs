{
   "compilerOptions": {
      "module": "commonjs",
      "declaration": false,
      "noImplicitAny": false,
      "noUnusedLocals": false,
      "removeComments": true,
      "noEmit": true,
      "noLib": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es6",
      "sourceMap": false,
      "allowJs": true,
      "lib": ["es7","dom"],
      "outDir": "./dist",
      "baseUrl": "./ts"
   },
   "exclude": ["node_modules"]
}