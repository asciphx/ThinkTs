{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "removeComments": true,
    "noEmit": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": false,
    "allowJs": true,
    "lib": ["es7","dom"],
    "outDir": "./dist",
    "baseUrl": "./ts"
  },
  "exclude": ["node_modules","log","lib","dist","routes","views"],
  "include": ["ts/**/*.ts"]
}